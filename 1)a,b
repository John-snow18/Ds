#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

struct node *head = NULL;

void create(int x) {
    struct node *newnode = malloc(sizeof(struct node));
    newnode->data = x;
    if (head == NULL) {
        head = newnode;
        newnode->next = head;
    } else {
        struct node *temp = head;
        while (temp->next != head)
            temp = temp->next;
        temp->next = newnode;
        newnode->next = head;
    }
}

void insert(int x, int pos) {
    struct node *newnode = malloc(sizeof(struct node));
    newnode->data = x;
    if (pos == 1) {
        struct node *temp = head;
        while (temp->next != head) temp = temp->next;
        newnode->next = head;
        temp->next = newnode;
        head = newnode;
    } else {
        struct node *temp = head;
        for (int i = 1; i < pos - 1; i++) temp = temp->next;
        newnode->next = temp->next;
        temp->next = newnode;
    }
}

void delete(int pos) {
    struct node *temp = head, *prev;

    if (pos == 1) {
        while (temp->next != head) temp = temp->next;
        struct node *del = head;
        head = head->next;
        temp->next = head;
        free(del);
    } else {
        temp = head;
        for (int i = 1; i < pos - 1; i++) temp = temp->next;
        prev = temp->next;
        temp->next = prev->next;
        free(prev);
    }
}

void traverse() {
    struct node *temp = head;
    if (head == NULL) return;
    do {
        printf("%d ", temp->data);
        temp = temp->next;
    } while (temp != head);
}

int main() {
    create(10);
    create(20);
    create(30);
    insert(5, 1);
    delete(2);
    traverse();
}
#include <stdio.h>
#define SIZE 50

int stack[SIZE], top = -1;

void push(int x) { stack[++top] = x; }
int pop() { return stack[top--]; }
void display() {
    for (int i = top; i >= 0; i--) printf("%d ", stack[i]);
}

int main() {
    push(10); push(20); push(30);
    pop();
    display();
}
