#include <stdio.h>

void merge(int a[], int l, int m, int r) {
    int n1 = m - l + 1, n2 = r - m;
    int L[n1], R[n2];

    for (int i = 0; i < n1; i++) L[i] = a[l + i];
    for (int j = 0; j < n2; j++) R[j] = a[m + 1 + j];

    int i = 0, j = 0, k = l;

    while (i < n1 && j < n2)
        a[k++] = (L[i] <= R[j]) ? L[i++] : R[j++];

    while (i < n1) a[k++] = L[i++];
    while (j < n2) a[k++] = R[j++];
}

void mergesort(int a[], int l, int r) {
    if (l < r) {
        int m = (l + r) / 2;
        mergesort(a, l, m);
        mergesort(a, m + 1, r);
        merge(a, l, m, r);
    }
}

int main() {
    int a[] = {12, 11, 13, 5, 6, 7};
    int n = 6;
    mergesort(a, 0, n - 1);
    for (int i = 0; i < n; i++) printf("%d ", a[i]);
}
#include <stdio.h>

int queue[50], front = -1, rear = -1;

void enqueue(int x) { if (front == -1) front = 0; queue[++rear] = x; }
int dequeue() { return queue[front++]; }

int main() {
    int n, a[10][10], visited[10] = {0};
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    int start = 0;
    enqueue(start);
    visited[start] = 1;

    while (front <= rear) {
        int v = dequeue();
        printf("%d ", v);

        for (int i = 0; i < n; i++)
            if (a[v][i] == 1 && !visited[i]) {
                enqueue(i);
                visited[i] = 1;
            }
    }
}
